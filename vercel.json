{
    "functions" : {
        "api/index.php" : {
            "runtime" : "vercel-php@0.7.1"
        }
    },
    "routes" : [
        {
            "src" : "/api/index.php",
            "dest" : "/api/index.php"
        },
        {
            "src": "/assets/(.*)",
            "dest": "/public/assets/$1"
        },
        {
            "src": "/public/(.*)",
            "dest": "/public/$1"
        },
        {
            "src" : "/assets/img/(.*).png",
            "dest" : "/public/assets/img/$1" 
        },
        {
            "src" : "/(.*)",
            "dest" : "/api/index.php"
        }
    ]
    
}