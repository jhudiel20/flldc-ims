{
    "functions" : {
        "api/index.php" : {
            "runtime" : "vercel-php@0.7.1"
        }
    },
    "routes" : [
        {
            "src" : "/api/index.php",
            "dest" : "/api/index.php"
        },
        {
            "src": "/assets/(.*)",
            "dest": "/public/assets/$1"
        },
        {   "src": "/(.*)", 
            "dest": "/public/$1" 
        },
        { "src": "/action/userlogin", "dest": "/public/action/userlogin.php" },
        {
            "src": "/assets/action/(.*)",
            "dest": "/public/assets/action/$1"
        },
        {
            "src": "/js/(.*)",
            "dest": "/public/js/$1"
        },
        {
            "src" : "/assets/img/(.*).png",
            "dest" : "/public/assets/img/$1" 
        },
        {
            "src" : "/(.*)",
            "dest" : "/api/index.php"
        }
    ]
    
}