{
    "functions": {
      "api/*.php": {
        "runtime": "vercel-php@0.7.1"
      },
      "api/action/*.php": {
        "runtime": "vercel-php@0.7.1"
      },
      "api/pages/*.php": {
        "runtime": "vercel-php@0.7.1"
      }
    },
    "routes": [
        {
            "src": "/acc-settings.php",
            "dest": "/pages/acc-settings.php"
          },
      {
        "src": "/userlogin.php",
        "dest": "/api/action/userlogin.php"
      },
      {
        "src": "/dashboard-lnd.php",
        "dest": "/api/pages/dashboard-lnd.php"
      },
      {
        "src": "/assets/(.*)",
        "dest": "/public/assets/$1"
      },
      {
        "src": "/css/(.*)",
        "dest": "/public/css/$1"
      },
      {
        "src": "/js/(.*)",
        "dest": "/public/js/$1"
      },
      {
        "src": "/(.*)",
        "dest": "/api/index.php"
      }
    ]
  }
  