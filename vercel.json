{
    "functions": {
      "api/*.php": {
        "runtime": "vercel-php@0.7.1"
      },
      "api/action/*.php": {
        "runtime": "vercel-php@0.7.1"
      },
      "api/pages/*.php": {
        "runtime": "vercel-php@0.7.1"
      },
      "api/ajax/*.php": {
        "runtime": "vercel-php@0.7.1"
      }
    },
    "routes": [
      {
        "src": "/user_image/(.*)",
        "dest": "/public/user_image/$1"
      },
      {
        "src": "/acc-settings.php",
        "dest": "/api/pages/acc-settings.php"
      },
      {
        "src": "/user_edit_info.php",
        "dest": "/api/action/user_edit_info.php"
      },
      {
        "src": "/upload_user_photo.php",
        "dest": "/api/action/upload_user_photo.php"
      },
      {
        "src": "/userlogin.php",
        "dest": "/api/action/userlogin.php"
      },
      {
        "src": "/Actions.php",
        "dest": "/api/action/Actions.php"
      },
      {
        "src": "/dashboard-lnd.php",
        "dest": "/api/pages/dashboard-lnd.php"
      },
      {
        "src": "/dashboard-eval.php",
        "dest": "/api/pages/dashboard-eval.php"
      },
      {
        "src": "/dashboard-lms.php",
        "dest": "/api/pages/dashboard-lms.php"
      },
      {
        "src": "/dashboard-mrs.php",
        "dest": "/api/pages/dashboard-mrs.php"
      },
      {
        "src": "/logs.php",
        "dest": "/api/pages/logs.php"
      },
      {
        "src": "/assets/(.*)",
        "dest": "/public/assets/$1"
      },
      {
        "src": "/css/(.*)",
        "dest": "/public/css/$1"
      },
      {
        "src": "/js/(.*)",
        "dest": "/public/js/$1"
      },
      {
        "src": "/(.*)",
        "dest": "/api/index.php"
      }
    ]
  }
  