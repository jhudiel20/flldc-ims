{
    "functions": {
      "api/*.php": {
        "runtime": "vercel-php@0.7.1"
      }
    },
    "routes": [
      {
        "src": "/api/action/(.*)",
        "dest": "/api/action/$1.php"
      },
      {
        "src": "/userlogin.php",
        "dest": "/api/action/userlogin.php"
      },
      {
        "src": "/assets/(.*)",
        "dest": "/public/assets/$1"
      },
      {
        "src": "/js/(.*)",
        "dest": "/public/js/$1"
      },
      {
        "src": "/css/(.*)",
        "dest": "/public/css/$1"
      },
      {
        "src": "/pages/(.*)",
        "dest": "/public/pages/$1"
      },
      {
        "src": "/(.*)",
        "dest": "/public/index.php"
      }
    ]
  }
  