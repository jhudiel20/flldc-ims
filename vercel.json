{
    "functions": {
      "api/*.php": {
        "runtime": "vercel-php@0.7.1"
      },
      "api/action/*.php": {
        "runtime": "vercel-php@0.7.1"
      },
      "api/pages/*.php": {
        "runtime": "vercel-php@0.7.1"
      },
      "api/ajax_data/*.php": {
        "runtime": "vercel-php@0.7.1"
      }
    },
    "routes": [
        {
            "src": "/DBConnection.php",
            "dest": "/api/DBConnection.php"
      }, 
      {
        "src": "/user_image/(.*)",
        "dest": "/public/user_image/$1"
      },
      {
        "src": "/img/(.*)",
        "dest": "/public/assests/img/$1"
      },
      {
        "src": "/acc-settings.php",
        "dest": "/api/pages/acc-settings.php"
      },
      {
        "src": "/user_edit_info.php",
        "dest": "/api/action/user_edit_info.php"
      },
      {
        "src": "/upload_user_photo.php",
        "dest": "/api/action/upload_user_photo.php"
      },
      {
        "src": "/userlogin.php",
        "dest": "/api/action/userlogin.php"
      },
      {
        "src": "/Actions.php",
        "dest": "/api/action/Actions.php"
      },
      {
        "src": "/dashboard-lnd.php",
        "dest": "/api/pages/dashboard-lnd.php"
      },
      {
        "src": "/dashboard-eval.php",
        "dest": "/api/pages/dashboard-eval.php"
      },
      {
        "src": "/dashboard-lms.php",
        "dest": "/api/pages/dashboard-lms.php"
      },
      {
        "src": "/dashboard-mrs.php",
        "dest": "/api/pages/dashboard-mrs.php"
      },
      {
        "src": "/request-list.php",
        "dest": "/api/pages/request-list.php"
      },
      {
        "src": "/request_list_data",
        "dest": "/api/ajax_data/request_list_data.php"
      },
      {
        "src": "/request-details.php",
        "dest": "/api/pages/request-details.php"
      },
      {
        "src": "/logs.php",
        "dest": "/api/pages/logs.php"
      },
      {
        "src": "/logs_data.php",
        "dest": "/api/ajax_data/logs_data.php"
      },
      {
        "src": "/user_list.php",
        "dest": "/api/pages/user_list.php"
      },
      {
        "src": "/user_list_data.php",
        "dest": "/api/ajax_data/user_list_data.php"  
      },
      {
        "src": "/api/pages/(.*)",
        "dest": "/api/pages/$1"
      },
      {
        "src": "/assets/(.*)",
        "dest": "/public/assets/$1"
      },
      {
        "src": "/css/(.*)",
        "dest": "/public/css/$1"
      },
      {
        "src": "/tabulator/(.*)",
        "dest": "/public/tabulator/$1"
      },
      {
        "src": "/(.*)",
        "dest": "/api/index.php"
      }
    ]
  }
  